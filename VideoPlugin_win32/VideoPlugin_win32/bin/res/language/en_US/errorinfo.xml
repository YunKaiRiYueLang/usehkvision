		<ErrorItem info="logon number is over max number">0xE460005</ErrorItem>
		<ErrorItem info="version is error">0xE460006</ErrorItem>
		<ErrorItem info="communication error with server">0xE460007,0xE460008,0xE460009,0xE46000a,0xE460012, 0xE46001f</ErrorItem>
		<ErrorItem info="data error">0xE46000b</ErrorItem>
		<ErrorItem info="call sequence or paramer error ">0xE46000c,0xE460011,0xE460020</ErrorItem>
		<ErrorItem info="command is over time ">0xE46000e</ErrorItem>
		<ErrorItem info="serial port error ">0xE46000f</ErrorItem>
