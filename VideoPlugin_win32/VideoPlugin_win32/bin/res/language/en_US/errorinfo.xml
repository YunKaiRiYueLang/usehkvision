		<ErrorItem info="has not to play">0xE410002 </ErrorItem> 
		<ErrorItem info="has no function">0xE410003, 0xE410007</ErrorItem> 
		<ErrorItem info="socket address reusing fail">0xE410004 </ErrorItem> 
		<ErrorItem info="save file failed">0xE410005 </ErrorItem> 
		<ErrorItem info="decoder called error">0xE410006 </ErrorItem> 
