		<ErrorItem info="open file failed">0x0E480900</ErrorItem>
		<ErrorItem info="operate has been cancled">0x0E480902 </ErrorItem>	
		<ErrorItem info="has no file">0x0E48090d，0x0E48090e</ErrorItem>		
		<ErrorItem info="can't to use GPU">0x0E480911 </ErrorItem>
		<ErrorItem info="can't get config">0x0E480912 </ErrorItem>
		<ErrorItem info="no path to save">0x0E480913</ErrorItem>

	</ErrorMode>
	
     <!-- http错误码-->
