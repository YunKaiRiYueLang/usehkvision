   image: url(:/GuiPublic/ComboBox/ComboBox_DateButton_dis.svg)
}
CDateEdit::drop-down,
CTimeEdit::drop-down,
CDateTimeEdit::drop-down,
CYearMonthDayEdit::drop-down,
DateTimePicker::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border-left-style: solid;
    border-left-color: darkgray;
    border-left-width: 0px;
	margin-right: 10px;
}

CTimeEdit::up-button,
CTimeEdit::down-button
{
	width: 0;
}

QLabel#time_bar_axis,
QLabel#time_bar_axis:hover,
QLabel#time_bar_axis:pressed,
QLabel#time_bar_axis:disabled
{
	background: transparent;
	border: none;
	background-image: url(:/VSSClient/Resources/playback_timebar_axis.svg);
}

