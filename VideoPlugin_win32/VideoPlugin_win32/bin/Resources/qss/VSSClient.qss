/* 菜单 */
QMenu
{
	background-color: white;
	border: 1px solid #e5e5e5;
	font: 12px Microsoft YaHei;
}

QMenu::item
{
	padding: 8px 24px;
	color: #333333;
}

QMenu::item:selected
{
	background-color: #f2f2f2;
}

QMenu::item:pressed
{
	background-color: #ebebeb;
}

QMenu::item:disabled
{
	color: #7f7f7f;
	background-color: white;
}

QMenu::indicator
{
	width: 16px;
	height: 16px;
	padding-left: 4px;
}

QMenu::indicator::non-exclusive:unchecked
{
	image: url(:/VSSClient/Resources/CS_base_icon_checkbox_nor.svg);
}

QMenu::indicator::non-exclusive:unchecked:selected
{
	image: url(:/VSSClient/Resources/CS_base_icon_checkbox_hover.svg);
}

QMenu::indicator::non-exclusive:checked
{
	image: url(:/VSSClient/Resources/CS_base_icon_checkbox_sel_01.svg);
}

QMenu::indicator::non-exclusive:checked:selected
{
	image: url(:/VSSClient/Resources/CS_base_icon_checkbox_sel_01.svg);
}

QMenu::indicator::exclusive:unchecked
{
	image: url(:/VSSClient/Resources/CS_base_icon_radiobox_nor.svg);
}

QMenu::indicator::exclusive:unchecked:selected
{
	image: url(:/VSSClient/Resources/CS_base_icon_radiobox_hover.svg);
}

QMenu::indicator::exclusive:checked
{
	image: url(:/VSSClient/Resources/CS_base_icon_radiobox_sel.svg);
}

QMenu::indicator::exclusive:checked:selected
{
	image: url(:/VSSClient/Resources/CS_base_icon_radiobox_sel.svg);
}

QMenu#save_as_menu,
QMenu#voice_talk_menu
{
	background-color: #3B3B3B;
	border: 1px solid #2a2a2a;
	font: 12px Microsoft YaHei;
}

QMenu#save_as_menu::item,
QMenu#voice_talk_menu::item
{
	padding: 8px 24px;
	color: #c0ffffff;
}

QMenu#save_as_menu::item:selected,
QMenu#voice_talk_menu::item:selected
{
	background-color: #1affffff;
	color: #ffffff;
}

QMenu#save_as_menu::item:pressed,
QMenu#voice_talk_menu::item:pressed
{
	background-color: #26000000;
	color: #ffffff;
}

QMenu#save_as_menu::item:disabled
{
	/* color: #7f7f7f; */
	/* background-color: white; */
}

/* 列表 */
iVMSGUIToolkit--CListView
{
	font: 13px Microsoft YaHei;
}

CModuleListView
{
	border: none;
	background-color: #262626;
