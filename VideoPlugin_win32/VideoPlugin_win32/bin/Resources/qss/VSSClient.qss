/*日历*/
CCalendarWidget,
CBeginEndCalendarWidget
{
    background-color: #F53E43;
	font: Microsoft YaHei;
}
CCalendarWidget
{
    border:1px solid #d2d2d2;
}

CCalendarWidget QLabel,
CBeginEndCalendarWidget QLabel
{
	color:rgb(255,255,255);
}
CBeginEndCalendarWidget QLabel#m_pLabelToday,
CBeginEndCalendarWidget QLabel#m_pBeginTimeLabel,
CBeginEndCalendarWidget QLabel#m_pEndTimeLabel
{
	color:rgb(0, 0, 0);
}
CCalendarWidget QToolButton#m_pPrevMonth,
CBeginEndCalendarWidget QToolButton#m_pPrevMonthBegin,
CBeginEndCalendarWidget QToolButton#m_pPrevMonthEnd
{
	border:none;
}
CCalendarWidget QToolButton#m_pNextMonth,
CBeginEndCalendarWidget QToolButton#m_pNextMonthBegin,
CBeginEndCalendarWidget QToolButton#m_pNextMonthEnd
{
	border:none;
}
/*前一年*/
CCalendarWidget iVMSGUIToolkit--CToolButton#m_pPrevMonth
{
    border: none;
	background: transparent;
	qproperty-m_normalIcon: url(:/GuiPublic/Calendar/Calendar_Last_Year_nor.png);
	qproperty-m_hoverIcon: url(:/GuiPublic/Calendar/Calendar_Last_Year_sel.png);
	qproperty-m_selectedIcon: url(:/GuiPublic/Calendar/Calendar_Last_Year_sel.png);
	qproperty-icon: url(:/GuiPublic/Calendar/Calendar_Last_Year_nor.png);
}
/*后一年*/
CCalendarWidget iVMSGUIToolkit--CToolButton#m_pNextMonth
{
    border: none;
	background: transparent;
	qproperty-m_normalIcon: url(:/GuiPublic/Calendar/Calendar_Next_Year_nor.png);
	qproperty-m_hoverIcon: url(:/GuiPublic/Calendar/Calendar_Next_Year_sel.png);
	qproperty-m_selectedIcon: url(:/GuiPublic/Calendar/Calendar_Next_Year_sel.png);
	qproperty-icon: url(:/GuiPublic/Calendar/Calendar_Next_Year_nor.png);
}

CCalendarView
{
	border-left: 1px solid #d2d2d2;
	border-right: 1px solid #d2d2d2;
	border-bottom: 1px solid #d2d2d2;
}
CCalendarView::item
{
    border:1px solid #fafafa;
}
CCalendarView::item:hover
{
	border: 1px solid #f53e43;
	color: #1e1e1e;
}
CCalendarView::item:selected
{
	background-color: #f53e43;
	color: #ffffff;
}

CCalendarWidget QFrame#m_pNavBarFrame,
CBeginEndCalendarWidget QWidget#m_pBeginHeader,
CBeginEndCalendarWidget QWidget#m_pEndHeader
{
	border-width:4px 2px 0 2px;
}
CCalendarTimeWidget#CCalendarTimeWidgetClassUi
{
	background-color:white;
}

CCalendarTimeWidget QFrame#m_pTimeBgFrame,
CBeginEndCalendarWidget QFrame#m_pBeginTimeBgFrame,
CBeginEndCalendarWidget QFrame#m_pEndTimeBgFrame
{
	background-color:white;
	border-left: 1px solid rgb(188,188,188);
	border-right: 1px solid rgb(188,188,188);
	border-bottom: 1px solid rgb(188,188,188);
}
CHourTimeEditPopup QTableWidget,
CMinuteTimeEditPopup QTableWidget
{
	border:hidden;
}
CHourTimeEditPopup QTableWidget::item:hover,
CMinuteTimeEditPopup QTableWidget::item:hover
{
    color:rgb(0,0,0);
} 
CHourTimeEditPopup QTableWidget::item:selected,
CMinuteTimeEditPopup QTableWidget::item:selected
{
	background-color:rgb(204,204,204);
    color:rgb(0,0,0);
} 
/*前一年*/
CYearTimeEditPopup iVMSGUIToolkit--CIconStatusButton#m_pPrev
{
	background-color: #f5f5f5;
	border-image: url(:/Public/Icon/Images/Public/Icon/btn_left_nor.svg);
}
/*前一年*/
CYearTimeEditPopup iVMSGUIToolkit--CIconStatusButton#m_pPrev:hover
{
	border-image:url(:/Public/Icon/Images/Public/Icon/btn_left_pre.svg);
	background-color: #e0e0e0;
}
/*后一年*/
CYearTimeEditPopup iVMSGUIToolkit--CIconStatusButton#m_pNext
{
	background-color: #f5f5f5;
	border-image: url(:/Public/Icon/Images/Public/Icon/btn_right_nor.svg);
}
/*后一年*/
CYearTimeEditPopup iVMSGUIToolkit--CIconStatusButton#m_pNext:hover
{
	border-image:url(:/Public/Icon/Images/Public/Icon/btn_right_pre.svg);
	background-color: #e0e0e0;
}
